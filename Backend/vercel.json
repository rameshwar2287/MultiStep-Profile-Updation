{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/user/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/country/(.*)",
        "dest": "/index.js"
      },{

        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }